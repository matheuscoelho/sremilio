<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegap.sremilio" version="1.3.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Sr. Em√≠lio</name>
    <description>
        A barbearia desde 1948.
    </description>
    <author email="contato@vipsio.com.br" href="http://vipsio.com.br">
        Vips.IO
    </author>
    <content src="index.html" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="StatusBarBackgroundColor" value="#ee6e73" />
    <preference name="StatusBarStyle" value="blacktranslucent" />


    <icon height="36" src="res\icon\36.png" width="36" />
    <icon height="48" src="res\icon\48.png" width="48" />
    <icon height="72" src="res\icon\72.png" width="72" />
    <icon height="96" src="res\icon\96.png" width="96" />
    <icon height="144" src="res\icon\144.png" width="144" />
    <icon height="192" src="res\icon\192.png" width="192" />
    <splash height="240" src="res\icon\240x320.png" width="320" />
    <splash height="320" src="res\icon\240x320.png" width="240" />

    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="phonegap-plugin-push" spec="^1.8.4">
        <variable name="SENDER_ID" value="249880936399" />
    </plugin>
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
</widget>
